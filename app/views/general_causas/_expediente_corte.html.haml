/ = render "general_causas/changes_table", changes: CausaChange.all

.row
	.col-md-12
		.grid.simple.can-collapse
			.grid-title
				%h4.semi-bold Expediente Corte
				.tools
					%a.marg-t-10.collapse
			.grid-body
				%table.table.table-striped.datatable.display.responsive.no-wrap
					%thead
						%tr
							%th Corte
							%th Libro
							%th Rol Ingreso
							%th Recurso							
					%tbody
						-unless change.nil?
							%tr
								%td
									%span
										=change.corte
								%td
									%span
										=change.libro
								%td
									%span
										=change.rol_ing
								%td
									%span
										=change.recurso
								