<% if @type == 0 %>
	$('#results').html("<%= escape_javascript (render :partial => 'results_laboral_table') %>");
<% elsif @type == 1 %> 
	$('#results').html("<%= escape_javascript (render :partial => 'results_civil_table') %>");
<% elsif @type == 2 %> 
	$('#results').html("<%= escape_javascript (render :partial => 'results_corte_table') %>");
<% elsif @type == 3 %> 
	$('#results').html("<%= escape_javascript (render :partial => 'results_procesal_table') %>");
<% elsif @type == 4 %> 
	$('#results').html("<%= escape_javascript (render :partial => 'results_suprema_table') %>");
<% end %>